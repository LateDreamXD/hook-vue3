{
  "manifest_version": 3,
  "name": "%name%",
  "version": "%version%",
  "description": "%description%",
  "permissions": ["scripting"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.iife.js"],
    "run_at": "document_start",
    "world": "MAIN"
  }],
  "icons": {
    "200": "icon.png"
  }
}
